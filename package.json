{
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "Japanese/English Flash Cards",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ./test/require.js ./test/unit/**/*.ts",
    "type": "tsc -b --verbose",
    "coverage": "nyc npm run test > '/dev/null'; nyc report",
    "b:clean": "rm -rf ./dist/*",
    "b:tag": "ts-node ./pwa/script/indexTagger.ts",
    "b:webpack": "webpack --mode=production",
    "b:manifest": "cp ./site/* ./dist/",
    "b:sw": "ts-node ./pwa/script/swBuilder.ts",
    "prebuild": "npm run b:clean && npm run b:tag",
    "build": "npm run b:webpack",
    "postbuild": "npm run b:manifest && npm run b:sw && rm index.production.html",
    "deploy": "gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "start": "webpack serve --mode=development --config=./webpack.config.js",
    "staged": "git diff --staged --diff-filter=d --name-only",
    "lint": "npm run -s staged | xargs eslint --fix",
    "lint:all": "DEBUG=*,-@hwc*,-eslint:code-path eslint .",
    "pretty": "npm run -s staged | xargs prettier --write"
  },
  "author": "Bryan",
  "license": "",
  "homepage": "https://bryanjimenez.github.io/nmemonica",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react-dom": "^1.3.0",
    "@mui/material": "^5.11.5",
    "@popperjs/core": "^2.11.6",
    "@primer/octicons-react": "^15.1.0",
    "@reduxjs/toolkit": "^1.9.1",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.1",
    "core-js": "^3.27.2",
    "firebase": "^9.16.0",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.41.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@testing-library/react": "^13.4.0",
    "@types/chai": "^4.3.5",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.4",
    "css-loader": "^6.2.0",
    "eslint": "^8.42.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "globals": "^13.20.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jsdom": "21.0.0",
    "jsdom-global": "3.0.2",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.2.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "sinon": "^15.0.3",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.52.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.1.0"
  }
}
