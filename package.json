{
  "private": true,
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "English - Japanese language learning app",
  "type": "module",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha -r ./test/require.js ./test/unit/**/*.ts",
    "type": "tsc -b --verbose",
    "coverage": "nyc npm run test > '/dev/null'; nyc report",
    "b:clean": "rm -rf ./dist/*",
    "b:tag": "ts-node ./pwa/script/indexTagger.ts",
    "b:rspack": "npm run b:clean && rspack build",
    "b:sw": "ts-node ./pwa/script/swBuilder.ts",
    "prebuild": "npm run b:clean && npm run b:tag",
    "build": "npm run b:rspack",
    "postbuild": "npm run b:sw && rm index.production.html",
    "deploy": "gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "start": "rspack serve",
    "staged": "git diff --staged --diff-filter=d --name-only",
    "lint": "npm run -s staged | xargs eslint --fix",
    "lint:all": "DEBUG=*,-@hwc*,-eslint:code-path eslint .",
    "pretty": "npm run -s staged | xargs prettier --write"
  },
  "author": "Bryan Jimenez",
  "license": "",
  "homepage": "https://github.com/bryanjimenez/nmemonica",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react-dom": "^2.0.2",
    "@mui/material": "^5.11.5",
    "@popperjs/core": "^2.11.6",
    "@primer/octicons-react": "^19.8.0",
    "@reduxjs/toolkit": "^1.9.1",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.1",
    "core-js": "^3.27.2",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.41.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@rspack/cli": "^0.3.8",
    "@testing-library/react": "^14.0.0",
    "@types/body-parser": "^1.19.4",
    "@types/chai": "^4.3.5",
    "@types/eslint": "^8.44.6",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/md5": "^2.3.2",
    "@types/mime": "^3.0.3",
    "@types/mocha": "^10.0.1",
    "@types/multiparty": "^0.0.36",
    "@types/node": "^20.8.9",
    "@types/prop-types": "^15.7.9",
    "@types/qs": "^6.9.9",
    "@types/range-parser": "^1.2.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/scheduler": "^0.16.5",
    "@types/send": "^0.17.3",
    "@types/sinon": "^10.0.15",
    "@types/sinonjs__fake-timers": "^8.1.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "chai": "^4.3.4",
    "eslint": "^8.42.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "globals": "^13.20.0",
    "jsdom": "22.1.0",
    "jsdom-global": "3.0.2",
    "license-checker-webpack-plugin": "^0.2.1",
    "md5": "^2.3.0",
    "mocha": "^10.2.0",
    "multiparty": "^4.2.3",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "react-refresh": "^0.14.0",
    "sinon": "^17.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  }
}
