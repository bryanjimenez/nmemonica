{
  "private": true,
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "English - Japanese language learning app",
  "type": "module",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha -r ./test/require.js ./test/unit/**/*.ts",
    "type": "tsc -b --verbose",
    "coverage": "nyc npm run test > '/dev/null'; nyc report",
    "b:clean": "rm -rf ./dist/*",
    "b:rspack": "npm run b:clean && rspack build",
    "b:docs": "./docs/mdbook build ./docs",
    "build": "npm run b:rspack",
    "predeploy": "npm run b:docs",
    "deploy": "gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "prestart": "npm run b:clean && NODE_ENV='development' npx rspack build -c ./rspack.config.sw.js",
    "start": "rspack serve",
    "staged": "git diff --staged --diff-filter=d --name-only",
    "lint": "npm run -s staged | xargs eslint --fix",
    "lint:all": "DEBUG=*,-@hwc*,-eslint:code-path eslint .",
    "pretty": "npm run -s staged | xargs prettier --write"
  },
  "author": "Bryan Jimenez",
  "license": "",
  "homepage": "https://github.com/bryanjimenez/nmemonica",
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanjimenez/nmemonica"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react-dom": "^2.1.3",
    "@mui/material": "^5.11.5",
    "@nmemonica/voice-en": "file:../nmemonica-wasm-voice-en/pkg/nmemonica-voice-en-0.0.1.tgz",
    "@nmemonica/voice-ja": "^0.1.4",
    "@nmemonica/x-spreadsheet": "^0.1.7",
    "@popperjs/core": "^2.11.6",
    "@primer/octicons-react": "^19.21.2",
    "@reduxjs/toolkit": "^2.11.2",
    "@zxing/browser": "^0.1.5",
    "bootstrap": "^5.3.6",
    "brotli-wasm": "^3.0.1",
    "classnames": "^2.3.1",
    "core-js": "^3.43.0",
    "crypto-browserify": "^3.12.1",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@rspack/cli": "^1.3.15",
    "@rspack/core": "^1.3.15",
    "@rspack/plugin-react-refresh": "^1.4.3",
    "@testing-library/react": "^16.3.0",
    "@types/chai": "^4.3.5",
    "@types/eslint": "^9.6.1",
    "@types/glob": "^8.1.0",
    "@types/license-checker-webpack-plugin": "^0.2.5",
    "@types/lodash": "^4.17.17",
    "@types/md5": "^2.3.5",
    "@types/mocha": "^10.0.10",
    "@types/n-readlines": "^1.0.6",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-redux": "^7.1.34",
    "@types/sinon": "^17.0.0",
    "@types/sinonjs__fake-timers": "^8.1.4",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "chai": "^4.3.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "jsdom-global": "3.0.2",
    "license-checker-webpack-plugin": "^0.2.1",
    "mocha": "^11.7.5",
    "n-readlines": "^1.0.1",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "nyc": "^17.1.0",
    "prettier": "^3.8.1",
    "react-refresh": "^0.17.0",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  }
}
