{
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "Japanese/English Flash Cards",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require @babel/register ./test/unit/**/*.js",
    "coverage": "nyc -e .jsx npm run test",
    "wdiochrome": "wdio wdio-chrome.conf.js",
    "b:clean": "rm -rf ./dist",
    "b:webpack": "node ./node_modules/webpack/bin/webpack.js --mode=production",
    "b:manifest": "cp ./site/* ./dist/",
    "b:sw": "node -r esm pwa/swHelper.js",
    "build": "npm run b:clean && npm run b:webpack && npm run b:manifest && npm run b:sw",
    "deploy": "./node_modules/.bin/gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "start": "webpack serve --mode=development --config=./webpack.config.js",
    "pretty": "./node_modules/.bin/prettier --write ./src/**/**/*.{js,jsx,css} ./test/**/*.js ./data ./webpack.config.js",
    "lint": "./node_modules/.bin/eslint --fix ./src/**/**/*.{js,jsx} ./pwa/*"
  },
  "author": "Bryan",
  "license": "",
  "homepage": "https://bryanjimenez.github.io/nmemonica",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@primer/octicons-react": "^15.1.0",
    "babel-loader": "^8.2.1",
    "bootstrap": "^4.6.0",
    "classnames": "^2.3.1",
    "core-js": "^3.17.2",
    "firebase": "^9.0.1",
    "lodash": "^4.17.20",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@babel/eslint-parser": "7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@wdio/cli": "^7.11.1",
    "@wdio/local-runner": "^7.11.1",
    "@wdio/mocha-framework": "^7.11.1",
    "@wdio/spec-reporter": "^7.10.1",
    "@wdio/sync": "^7.11.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "chai": "^4.3.4",
    "chromedriver": "^106.0.1",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "glob-all": "^3.2.1",
    "html-loader": "^2.1.2",
