{
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "Japanese/English Flash Cards",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ./test/require.js ./test/unit/**/*.js",
    "type": "tsc -b --verbose",
    "coverage": "nyc -e .jsx npm run test",
    "b:clean": "rm -rf ./dist/*",
    "b:tag": "ts-node --esm ./pwa/script/indexTagger.ts",
    "b:webpack": "webpack --mode=production",
    "b:manifest": "cp ./site/* ./dist/",
    "b:sw": "ts-node --esm ./pwa/script/swBuilder.ts",
    "prebuild": "npm run b:clean && npm run b:tag",
    "build": "npm run b:webpack",
    "postbuild": "npm run b:manifest && npm run b:sw && rm index.production.html",
    "deploy": "gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "start": "webpack serve --mode=development --config=./webpack.config.js",
    "staged": "git diff --staged --diff-filter=d --name-only",
    "lint": "npm run -s staged | xargs eslint --fix",
    "lint:all": "DEBUG=*,-@hwc*,-eslint:code-path eslint .",
    "pretty": "npm run -s staged | xargs prettier --write"
  },
  "author": "Bryan",
  "license": "",
  "homepage": "https://bryanjimenez.github.io/nmemonica",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react-dom": "^1.3.0",
    "@mui/material": "^5.11.5",
    "@popperjs/core": "^2.11.6",
    "@primer/octicons-react": "^15.1.0",
    "@reduxjs/toolkit": "^1.9.1",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.1",
    "core-js": "^3.27.2",
    "firebase": "^9.16.0",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@eslint/js": "^8.41.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/md5": "^2.3.2",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.27",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "chai": "^4.3.4",
    "css-loader": "^6.2.0",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "globals": "^13.20.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jsdom": "21.0.0",
    "jsdom-global": "3.0.2",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.2.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "sinon": "^15.0.3",
    "style-loader": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.52.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.1.0"
  }
}
