{
  "name": "nmemonica",
  "version": "1.0.0",
  "description": "Japanese/English Flash Cards",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ./test/require.js ./test/unit/**/*.js",
    "type": "tsc --noEmit -p ./tsconfig.json",
    "coverage": "nyc -e .jsx npm run test",
    "wdiochrome": "wdio wdio-chrome.conf.js",
    "b:clean": "rm -rf ./dist/* ./pwa/built/*",
    "b:helper": "tsc -p ./pwa/tsconfig.json",
    "b:tag": "cp index.html indexOriginal.temp && node ./pwa/built/pwa/src/indexHelper.js",
    "b:webpack": "node ./node_modules/webpack/bin/webpack.js --mode=production",
    "b:unTag": "mv indexOriginal.temp index.html",
    "b:manifest": "cp ./site/* ./dist/",
    "b:sw": "node ./pwa/built/pwa/src/swHelper.js",
    "build": "npm run b:clean && npm run b:helper && npm run b:tag && npm run b:webpack && npm run b:manifest && npm run b:sw ; npm run b:unTag",
    "deploy": "./node_modules/.bin/gh-pages -d dist -r git@github.com:bryanjimenez/nmemonica -b gh-pages",
    "start": "webpack serve --mode=development --config=./webpack.config.js",
    "staged": "git diff --staged --diff-filter=d --name-only",
    "lint": "npm run staged | egrep '^.*(.js|.jsx)$' | xargs ./node_modules/.bin/eslint --fix",
    "pretty": "npm run staged | egrep '^.*(.ts|.css)$' | xargs ./node_modules/.bin/prettier --write"
  },
  "author": "Bryan",
  "license": "",
  "homepage": "https://bryanjimenez.github.io/nmemonica",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react-dom": "^1.3.0",
    "@mui/material": "^5.11.5",
    "@popperjs/core": "^2.11.6",
    "@primer/octicons-react": "^15.1.0",
    "@reduxjs/toolkit": "^1.9.1",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.1",
    "core-js": "^3.27.2",
    "firebase": "^9.16.0",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/react": "^18.0.27",
    "@wdio/cli": "^7.11.1",
    "@wdio/local-runner": "^7.11.1",
    "@wdio/mocha-framework": "^7.11.1",
    "@wdio/spec-reporter": "^7.10.1",
    "@wdio/sync": "^7.11.1",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "chai": "^4.3.4",
    "chromedriver": "^88.0.0",
    "css-loader": "^6.2.0",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "glob-all": "^3.2.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jsdom": "21.0.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^2.2.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "sinon": "^15.0.3",
    "style-loader": "^3.2.1",
    "typescript": "^4.8.4",
    "webpack": "^5.52.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.1.0"
  }
}
